// Escribir un programa que convierta un valor dado en grados Fahrenheit a grados Celsius.

Algoritmo ConversorGrados
	
	Definir  grados_C , grados_f Como Real
	
	Escribir  "Ingrese grados en Fahrenheit :"
	leer grados_f
	
	grados_C = (grados_f-32)*(5/9)
	
	
	Escribir  " grados en celsius :",grados_C,"C°"
	
	
FinAlgoritmo
